span> ArcNode&#123;<br><span class="hljs-keyword">int</span> adjvex;<br><span class="hljs-keyword">struct</span> ArcNode *nextarc;<br><span class="hljs-keyword">int</span> info;<br>&#125;ArcNode;<br><span class="hljs-keyword">typedef</span> <span class="hljs-keyword">struct</span> VNode&#123;<br>ArcNode *firstarc;<br><span class="hljs-keyword">char</span> data;<br>&#125;VNode;<br><span class="hljs-keyword">typedef</span> <span class="hljs-keyword">struct</span> AGraph&#123;<br>VNode adjlist[maxSize];<br><span class="hljs-keyword">int</span> e,n;<br><br>&#125;AGraph;<br><br>AGraph buildAGraph()<br>&#123;<br>AGraph ag;<br>ag.n=<span class="hljs-number">6</span>;<br>ag.e=<span class="hljs-number">7</span>;<br><span class="hljs-keyword">int</span> i;<br>VNode vn;<br><span class="hljs-keyword">for</span>(i=<span class="hljs-number">0</span>;i&lt;ag.n;i++)<br>&#123;<br>vn.data=(<span class="hljs-keyword">char</span>)(<span class="hljs-number">65</span>+i);<br><span class="hljs-comment">//printf("%c",vn.data);</span><br>ag.adjlist[i]=vn;<br>&#125;<br>ArcNode arc1,arc2,arc3,arc4,arc5,arc6,arc7;<br>arc1.adjvex=<span class="hljs-number">1</span>;<br>arc1.nextarc=&amp;arc3;<br>arc2.adjvex=<span class="hljs-number">2</span>;<br>arc2.nextarc=&amp;arc4;<br><br>arc3.adjvex=<span class="hljs-number">3</span>;<br>arc4.adjvex=<span class="hljs-number">4</span>;<br>        arc5.adjvex=<span class="hljs-number">4</span>;<br>arc6.adjvex=<span class="hljs-number">3</span>;<br>        arc7.adjvex=<span class="hljs-number">5</span>;<br>arc3.nextarc=<span class="hljs-literal">NULL</span>;<br>arc4.nextarc=<span class="hljs-literal">NULL</span>;<br>arc5.nextarc=<span class="hljs-literal">NULL</span>;<br>arc6.nextarc=<span class="hljs-literal">NULL</span>;<br>arc7.nextarc=<span class="hljs-literal">NULL</span>;<br>ag.adjlist[<span class="hljs-number">0</span>].firstarc=&amp;arc1;<br>ag.adjlist[<span class="hljs-number">1</span>].firstarc=&amp;arc2;<br>ag.adjlist[<span class="hljs-number">2</span>].firstarc=<span class="hljs-literal">NULL</span>;<br>ag.adjlist[<span class="hljs-number">3</span>].firstarc=&amp;arc5;<br>ag.adjlist[<span class="hljs-number">4</span>].firstarc=&amp;arc7;<br>ag.adjlist[<span class="hljs-number">5</span>].firstarc=&amp;arc6;<br><span class="hljs-keyword">return</span> ag;<br>&#125;<br><br><span class="hljs-keyword">int</span> visit[]=&#123;<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>&#125;;<br><span class="hljs-keyword">void</span> DFS(AGraph *G,<span class="hljs-keyword">int</span> v)<br>&#123;<br><span class="hljs-keyword">char</span> content=G-&gt;adjlist[v].data;<br>printf(<span class="hljs-string">"%c"</span>,content);<br>visit[v]=<span class="hljs-number">1</span>;<br>ArcNode *p=G-&gt;adjlist[v].firstarc;<br><span class="hljs-keyword">while</span>(p!=<span class="hljs-literal">NULL</span>)<br>&#123;<br><span class="hljs-keyword">if</span>(visit[p-&gt;adjvex]==<span class="hljs-number">0</span>)<br>&#123;<br>DFS(G,p-&gt;adjvex);<br>&#125;<br>p=p-&gt;nextarc;<br>&#125;<br><br><br>&#125;<br><span class="hljs-keyword">int</span> Visit[]=&#123;<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>,<span class="hljs-number">0</span>&#125;;<br><span class="hljs-keyword">void</span> BFS(AGraph *G,<span class="hljs-keyword">int</span> v)<br>&#123;<br><span class="hljs-keyword">char</span> content=G-&gt;adjlist[v].data;<br>printf(<span class="hljs-string">"%c"</span>,content);<br>Visit[v]=<span class="hljs-number">1</span>;<br><span class="hljs-keyword">int</span> que[maxSize],rear=<span class="hljs-number">0</span>,front=<span class="hljs-number">0</span>;<br><span class="hljs-keyword">int</span> k;<br><span class="hljs-keyword">int</span> i,j;<br>rear=(rear+<span class="hljs-number">1</span>)%maxSize;<br>que[rear]=v;<br>ArcNode *p;<br><span class="hljs-keyword">while</span>(rear!=front)<br>&#123;<br>front=(front+<span class="hljs-number">1</span>)%maxSize;<br>k=que[front];<br>p=G-&gt;adjlist[k].firstarc;<br> <span class="hljs-keyword">while</span>(p!=<span class="hljs-literal">NULL</span>)<br> &#123;<br><span class="hljs-keyword">if</span>(Visit[p-&gt;adjvex]==<span class="hljs-number">0</span>)<br>&#123;<br>content=G-&gt;adjlist[p-&gt;adjvex].data;<br>printf(<span class="hljs-string">"%c"</span>,content);<br>Visit[p-&gt;adjvex]=<span class="hljs-number">1</span>;<br>rear=(rear+<span class="hljs-number">1</span>)%maxSize;<br>que[rear]=p-&gt;adjvex;<br>&#125;<br>p=p-&gt;nextarc;<br> &#125;<br>&#125;<br><br>&#125;<br><span class="hljs-keyword">void</span> main()<br>&#123;<br>AGraph ag=buildAGraph();<br>DFS(&amp;ag,<span class="hljs-number">0</span>);<br>printf(<span class="hljs-string">"\n"</span>);<br>BFS(&amp;ag,<span class="hljs-number">0</span>);<br>&#125;<br></code></pre></td></tr></table></figure>]]></content>
      
